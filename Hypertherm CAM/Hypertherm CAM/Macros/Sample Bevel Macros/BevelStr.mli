
;
; This lead-in macro should be used on a corner
;
START
GOTO 3 1.25
START_LEADIN
INSERT (START LEAD-IN, BevelStr.mli)
GOTO 3 1.0
SETVOLTAGE
SETKERF
KERFLEFT
AHCON
	IF ENDANGLE ! 0
		ROTATEON
		FOLLOWERON
	ENDIF
FEEDRATE
